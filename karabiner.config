#!/bin/sh

cli=/Applications/Karabiner.app/Contents/Library/bin/karabiner

$cli set private.german_neo_bothshift_to_capslock 1
/bin/echo -n .
$cli set private.german_neo_capslock_to_option_L 1
/bin/echo -n .
$cli set private.german_neo_mod4 1
/bin/echo -n .
$cli set remap.german_neo_mod3 1
/bin/echo -n .
$cli set repeat.initial_wait 200
/bin/echo -n .
$cli set repeat.wait 30
/bin/echo -n .
/bin/echo
